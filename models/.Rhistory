filter(tree_num == 171)
} else if(nrow(data) == 676*50) {
data <- data %>%
filter(tree_num == 351)
} else if(nrow(data) == 961*50) {
data <- data %>%
filter(tree_num == 481)
} else if(nrow(data) == 1225*50) {
data <- data %>%
filter(tree_num == 613)
}
}
